@model System.Web.Mvc.HandleErrorInfo

@{
    Layout = null;
}

@Styles.Render("~/bundles/bootstrap")
@Styles.Render("~/bundles/admin-lte")
@Styles.Render("~/bundles/admin-lte/skins")
@Styles.Render("~/Content/css")
@Scripts.Render("~/bundles/modernizr")

<title>Antiguera - @Response.StatusCode.ToString() Não Encontrado</title>

<div class="container">
    <div class="row">
        <img class="img-responsive" style="margin: 50px auto" width="400" src="~/Content/Images/LogoAntiguera2.png" />
    </div>
    <div class="row">
        <h1 class="text-center">@Response.StatusCode! Página não encontrada!</h1>
    </div>
</div>

@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/js/bootstrap")
